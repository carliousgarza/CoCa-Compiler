program p;
var
int: i;

function int fibonacci(int o)
{
   if (o<0) then {
      print("Incorrect input");
      return (-1);
   }
   if (o==1) then {
      return (0);
    }
   if (o==2) then {
      return (1);
   }
   return (fibonacci(o-1)+fibonacci(o-2));
}

main()
{
  i = fibonacci(15);
  print(i);
}
