program littleDuck2020CorrectFileTest;
var int: i, j;

function int fibonacci (int n)
var int: first, second, i, sum;
{
  i = 0;
  sum = 0;
  first = 0;
  second = 1;

  from i=0 to n do {
    if (i <= 1) then {
      sum = i;
    }
    else {
      sum = first + second;
      first = second;
      second = sum;
    }
  }
  return(sum);
}

main()
{
  j = 30;
  i = fibonacci(30);
  print(i);
}
